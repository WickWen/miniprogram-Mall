/**app.wxss**/
@import"./styles/iconfont.wxss";
/* 布局测试使用 */
image{
    background-color: #eee;
}

/* Css3 变量 */
 page {
  --MainColor: #EA4350;
  --PrimaryColor: #409EFF;
  --SuccessColor: #67C23A;
  --WarningColor: #FCAA23;
  --DangerColor: #F56C6C;
  --PrimaryText: #303133;
  --IconColor: #434343;
  --bgColor: #f9f9f9;
  --divideHeight: 20rpx;
}

.divide{
  height: var(--divideHeight);
  background-color: #f9f9f9;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
